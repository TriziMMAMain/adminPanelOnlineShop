{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/yarn",
      "config": {
        "optimize": {
          "include": [
            "vue",
            "lodash"
          ]
        },
        "rollupOutputOptions": {
          "external": ["fs"]
        },
        "devCommand": "yarn dev",
        "buildCommand": "yarn build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}